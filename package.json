{
  "name": "webpack-vue-todo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js",
    "clean": "rimraf dist && rimraf assets",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "nodemon server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "practice": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.practice.js",
    "practice:build": "cross-env NODE_ENV=production webpack --config build/webpack.config.practice.client.js",
    "clean:practice": "rimraf program"
  },
